{
    "version": 2,
    "builds": [
        {
            "src": "app_rapid_api.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app_rapid_api.py"
        }
    ]
} 